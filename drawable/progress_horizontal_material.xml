<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill_horizontal|center" android:id="@android:id/background">
        <shape android:shape="rectangle">
            <size android:height="6.0dip" />
            <solid android:color="#0033b5e5" />
            <corners android:radius="2.0dip" />
            <stroke android:width="1.0dip" android:color="#ff33b5e5" />
        </shape>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <shape android:shape="rectangle">
                <size android:height="6.0dip" />
                <corners android:radius="2.0dip" />
                <solid android:color="#ff33b5e5" />
            </shape>
        </scale>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@id/progress">
        <scale android:scaleWidth="100%">
            <shape android:shape="rectangle">
                <size android:height="6.0dip" />
                <corners android:radius="2.0dip" />
                <solid android:color="#ff33b5e5" />
            </shape>
        </scale>
    </item>
</layer-list>