<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="fill_horizontal|center" android:id="@android:id/background">
        <shape android:shape="rectangle">
            <size android:height="6.0dip" />
            <gradient android:startColor="#00000000" android:endColor="#00000000" />
            <corners android:radius="2.0dip" />
            <stroke android:width="1.0dip" android:color="#ff33b5e5" />
        </shape>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@android:id/secondaryProgress">
        <scale android:scaleWidth="100%">
            <selector>
                <item android:state_enabled="true" android:drawable="@android:color/transparent" />
                <item>
                    <shape android:shape="rectangle">
                        <size android:height="6.0dip" />
                        <corners android:radius="2.0dip" />
                        <stroke android:width="1.0dip" android:color="#ff33b5e5" />
                    </shape>
                </item>
            </selector>
        </scale>
    </item>
    <item android:gravity="fill_horizontal|center" android:id="@android:id/progress">
        <scale android:scaleWidth="100%">
            <selector>
                <item android:state_enabled="true" android:drawable="@android:color/transparent" />
                <item>
                    <shape android:shape="rectangle">
                        <size android:height="6.0dip" />
                        <corners android:radius="2.0dip" />
                        <stroke android:width="1.0dip" android:color="#ff33b5e5" />
                    </shape>
                </item>
            </selector>
        </scale>
    </item>
</layer-list>